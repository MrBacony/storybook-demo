{
  "npmScope": "storybook",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "storybook-demo": {
      "tags": []
    },
    "storybook-demo-e2e": {
      "tags": [],
      "implicitDependencies": [
        "storybook-demo"
      ]
    },
    "ui": {
      "tags": []
    }
  }
}
